    <body>
      <div type="book">
        <linkGrp type="notes">
          <link target="#spp-098-45L4-footnote-1 #spp-098-45L4-line-18"/>
          <link target="#spp-098-45L4-footnote-2 #spp-098-45L4-line-30"/>
