      <div type="book">
        <linkGrp type="notes">
          <link target="#spp-284-06E2-footnote-1 #spp-284-06E2-line-6"/>
          <link target="#spp-284-06E2-footnote-2 #spp-284-06E2-line-7"/>
          <link target="#spp-284-06E2-footnote-3 #spp-284-06E2-line-8"/>
