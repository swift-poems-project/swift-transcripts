      <div type="book">
        <linkGrp type="notes">
          <link target="#spp-980-15W1-footnote-title-1 #spp-980-15W1-title-1"/>
          <link target="#spp-980-15W1-footnote-1 #spp-980-15W1-line-7"/>
          <link target="#spp-980-15W1-footnote-2 #spp-980-15W1-line-16"/>
          <link target="#spp-980-15W1-footnote-3 #spp-980-15W1-line-16"/>
